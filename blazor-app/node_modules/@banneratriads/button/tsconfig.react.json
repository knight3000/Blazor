  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "outDir": "./react", 
    "declaration": false,
    "jsx": "react",
    "esModuleInterop": true,
    "allowJs": true,  
    "skipLibCheck": true,
    "moduleResolution": "node"
  },
  "include": ["react/**/*.ts"],
  "exclude": ["node_modules"]
}
